{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "localhost (Chrome)",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}",
      "resolveSourceMapLocations": [
          "${workspaceFolder}/**",
          "!**/node_modules/**"
      ],
      "preLaunchTask": "ng serve"
    }
  ]
}
